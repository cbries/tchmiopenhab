{
  "$schema": "http://json.schemastore.org/tsconfig",
  "compileOnSave": true,
  "compilerOptions": {
    "module": "none",
    "target": "es6",
    "skipLibCheck": true,
    "lib": [
      "dom",
      "scripthost",
      "es6",
      "es2017.intl"
    ],
    "types": [
      
    ],
    "declaration": true,
    "sourceMap": true,
    "noEmitOnError": true,
    "suppressImplicitAnyIndexErrors": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictBindCallApply": true,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "strictFunctionTypes": false,
    "strictPropertyInitialization": false,
    "alwaysStrict": false
  },
  "include": [
    "Libs/moment.d.ts",
    "$(Beckhoff.TwinCAT.HMI.Framework).InstallPath/TcHmi.d.ts",
    "$(Beckhoff.TwinCAT.HMI.Framework).InstallPath/Controls/System/TcHmiRegion/TcHmiRegion.d.ts",
    "$(Beckhoff.TwinCAT.HMI.Controls).InstallPath/TcHmiImage/TcHmiImage.d.ts",
    "$(Beckhoff.TwinCAT.HMI.Controls).InstallPath/TcHmiButton/TcHmiButton.d.ts",
    "$(Beckhoff.TwinCAT.HMI.Controls).InstallPath/TcHmiComboBox/TcHmiComboBox.d.ts",
    "$(Beckhoff.TwinCAT.HMI.Controls).InstallPath/TcHmiCheckBox/TcHmiCheckBox.d.ts",
    "$(Beckhoff.TwinCAT.HMI.Controls).InstallPath/TcHmiToggleSwitch/TcHmiToggleSwitch.d.ts",
    "$(Beckhoff.TwinCAT.HMI.Controls).InstallPath/TcHmiTachometer/TcHmiTachometer.d.ts"
  ],
  "files": [
    "BaseControlTs/BaseControlTs.ts",
    "BatteryTs/BatteryTs.ts",
    "BlindsTs/BlindsTs.ts",
    "ContactDoorTs/ContactDoorTs.ts",
    "ContactWindowTs/ContactWindowTs.ts",
    "DaylightTs/global.d.ts",
    "DaylightTs/DaylightTs.ts",
    "LampSwitchTs/LampSwitchTs.ts",
    "SensorHeatingTs/SensorHeatingTs.ts",
    "SensorHumidityTs/SensorHumidityTs.ts",
    "SensorTemperatureTs/SensorTemperatureTs.ts",
    "StateTs/StateTs.ts",
    "SurveillanceTs/SurveillanceTs.ts",
    "SwipeRegionTs/global.d.ts",
    "SwipeRegionTs/SwipeRegionTs.ts",
    "SwitchTs/SwitchTs.ts"
  ]
}